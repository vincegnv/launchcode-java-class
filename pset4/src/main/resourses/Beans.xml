<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="mario" class="com.vincegnv.pset4.Mario">
        <constructor-arg name="shapeBuilder" ref="shapeBuilder"/>
        <constructor-arg name="printer" ref="printer"/>
    </bean>

    <bean id="shapeBuilder" class="com.vincegnv.pset4.ShapeBuilder"></bean>

    <bean id="printer" class="com.vincegnv.pset4.Printer"></bean>

</beans>