<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="greedy" class="com.vincegnv.pset5.Greedy">
        <property name="validator" ref="inputValidator"></property>
        <property name="coinCalculator" ref="coinCalculator"></property>
    </bean>
    
    <bean id="inputValidator" class="com.vincegnv.pset5.InputValidator"></bean>
    
    <bean id="coinCalculator" class="com.vincegnv.pset5.CoinCalculator"></bean>

</beans>